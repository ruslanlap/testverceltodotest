{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-node@18.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/todos",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/api/todos"
    },
    {
      "src": "/api/todos/(.*)",
      "methods": ["DELETE", "PATCH", "OPTIONS"],
      "dest": "/api/todos/$1"
    }
  ]
}
